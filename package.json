{
  "name": "vscode-pygyat",
  "displayName": "vscode-pygyat",
  "description": "PyGyat support for VS Code",
  "version": "1.0.0",
  "publisher": "shamith-pasula",
  "engines": {
    "vscode": "^1.95.0"
  },
  "icon": "./icons/pygyat.png",
  "repository": "https://github.com/shamith09/vscode-pygyat",
  "readme": "./README.md",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pygyat",
        "aliases": [
          "PyGyat",
          "pygyat"  
        ],
        "extensions": [
          ".gyat"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/pygyat.png",
          "dark": "./icons/pygyat.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "pygyat",
        "scopeName": "source.pygyat",
        "path": "./syntaxes/PyGyat.tmLanguage"
      }
    ]
  }
}
